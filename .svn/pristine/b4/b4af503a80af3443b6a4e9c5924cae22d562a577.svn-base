
.txt{
  font-weight: 700;
   font-size: 36rpx;
  line-height: 50rpx;
  color: #000;
}

/* 顶部导航  */
.show-bar{
  width: 100%;
  background-color: #fff;
  display: flex;
  flex-flow: nowrap;
  margin-left: 54rpx;
  font-size: 32rpx;
  line-height: 44rpx;
  font-weight:400;
  color: rgba(0, 0, 0, 0.2);
 position:fixed;
 
}

.course-classify{
  height: 90rpx;
  width: 100%;
  line-height: 90rpx;
  display: flex;
  justify-content:space-around;
  background-color: #fff;
  position: fixed;
  top: 64px;
  left: 0;
  z-index: 223;
}
.course-classify-item{
  font-size: 32rpx;
  color: rgba(0, 0, 0, .2);
  letter-spacing: 1rpx;
}
.active{
  border-bottom: 6rpx solid #000;
  color: #000;
}



.cur{
  border-bottom: 6rpx solid #000;
  color:rgba(0,0,0,1);font-weight:600;
}
.city_list{
   margin-top: 30rpx;
   margin-left: 30rpx;
   margin-right: 30rpx;
}

.left{
   display: inline-block;
   width: 336rpx;
   padding-right: 18rpx;
   
}
.right{
  float: right;
  width: 336rpx;
}
.left-message{
  width: 336rpx;
  height:456rpx;
  border-radius: 16rpx;
  background-color: #fff;
  padding-top: 8rpx;
  padding-left: 8rpx;
  padding-right: 8rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  box-shadow: 4rpx 2rpx 30px 0 rgba(89, 89, 89, 0.15); 
}
.right-message .message:not(:first-child){
  width: 336rpx;
  height:456rpx;
  border-radius: 16rpx;
  background-color: #fff;
  padding-top: 8rpx;
  padding-left: 8rpx;
  padding-right: 8rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
   box-shadow: 4rpx 2rpx 30px 0 rgba(89, 89, 89, 0.15);
}
.right-message .message:first-child .item-img{
  width: 336rpx;
  height: 348rpx;
  margin-bottom: 20rpx;
  box-shadow: 4rpx 2rpx 30px 0 rgba(89, 89, 89, 0.15);
}
.picture{
  width: 336rpx;
 
  border-radius: 16rpx;
  box-shadow: 4rpx 2rpx 30px 0 rgba(89, 89, 89, 0.15);
}
.picture .item-img{
   width: 336rpx;
  height: 348rpx;
}
.city_item{
  position: relative;
}
.item-img{
  width: 320rpx;
  height: 380rpx;
  border-radius: 16rpx;
  display: block;
}
.contents{
  padding: 16rpx 20rpx 20rpx 16rpx;
  box-sizing: border-box;
   font-size: 22rpx;
   line-height: 32rpx;
   font-weight:400;
   align-items: center;
   
    /*  */
}
.city_site{
  width:24rpx;
  height:24rpx;
  padding-right:4rpx;
 vertical-align:middle;
  
}
.activity-cityTime{
  width: 100%;
 display: flex;
  justify-content: space-between;
}
.activity-city{
   color: #000;
  
}

.activity-time{
  color: rgba(0, 0, 0, 0.3);
}
.activity-state{
   position:absolute;
   left:30rpx;
   top:224rpx;
   height: 34rpx;
   border-radius: 4rpx;
   padding-left: 10rpx;
   padding-right: 10rpx;
   color: #fff;
   font-weight:400;
   font-size: 20rpx;
   background-color: rgba(48, 193, 255, 1); 
}
.activity-title{
position:absolute;
   left:30rpx;
   top:266rpx;
   padding-bottom: 10rpx;
   width: 270rpx;
   /* height: 72rpx; */
   font-weight:600;
   font-size: 36rpx;
   line-height: 50rpx;
   color: #fff;
   display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;
   overflow: hidden; text-overflow:ellipsis;
}

 /* 更多活动  */
.activity-more{
