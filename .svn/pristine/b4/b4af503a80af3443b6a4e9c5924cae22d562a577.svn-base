  font-size: 16rpx;
  line-height: 20rpx;
   color: rgba(255, 255, 255, .3)
}
.more-string{
  color: #fff;
  font-size: 40rpx;
  line-height: 54rpx;
  margin-bottom: 64rpx;
  font-weight: 600;
}
.show_down{
  width: 168rpx;
