height: 456rpx;
border-radius: 16rpx;
background-color: #fff;
padding-top: 8rpx;
padding-left: 8rpx;
padding-right: 8rpx;
box-sizing: border-box;
margin-bottom: 20rpx;
box-shadow: 4rpx 2rpx 30px 0 rgba(89, 89, 89, 0.15);
/* align-content: space-between; */
