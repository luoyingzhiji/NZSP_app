/*评论样式*/
.add-comment-wrap{z-index:1;background: #000;opacity: .3;filter: alpha(opacity=30);position: fixed;height: 100%;width: 100%;top: 0;right: 0;bottom: 0;left: 0;}
.add-comment{position: fixed;bottom:0;background:#fff;width: 710rpx;padding:20rpx;z-index: 100;}
.add-comment textarea{background:#ddd;width: 690rpx;height:160rpx;border-radius: 16rpx;font-size: 30rpx;padding:10rpx;}
.comment-button{text-align: right;margin-top:10rpx;}
.comment-button .txt{padding:10rpx 20rpx;color:#fff;font-size:26rpx;background: #1DA2FF;border-radius: 8rpx;width:100rpx;display:inline;}
.comment-button .cancel{background: #ddd;}
/* 评论样式适配iPhoneX */
.add-comment-iphoneX{bottom: 68rpx;}
/*提示框*/
.dialog-wrap{width:240rpx;text-align: center;background: #fff;
    box-shadow: rgba(7, 17, 27, 0.1) 0px 4px 8px 0px;z-index: 99999;left: 50%; margin: 0 0 0 -165rpx; position: fixed; top: 24%;border-radius: 8rpx;padding: 24rpx 40rpx 40rpx;}
.dialog-wrap image{display: block;width: 21rpx;height:34rpx;margin:20rpx auto;}
.dialog-wrap .txt{font-size: 26rpx;color:#999;}

/*授权信息框*/
.check-dialog-wrap{width:500rpx;text-align: center;background: #fff;
    box-shadow: rgba(7, 17, 27, 0.1) 0px 4px 8px 0px;z-index: 99999;left: 50%; margin: 0 0 0 -285rpx; position: fixed; top: 24%;border-radius: 8rpx;padding: 24rpx 40rpx 40rpx;}
.check-dialog-wrap .content{font-size: 30rpx;line-height: 40rpx;text-align: left;color:#666;padding:50rpx 20rpx 30rpx;}
.check-dialog-wrap button{font-size: 32rpx;color:#14BA4C;background: #fff;}
.check-dialog-wrap button::after{ border: none; }

button.form_button{background-color:transparent;padding:0;margin:0;display:inline;position:static;border:0;padding-left:0;padding-right:0;border-radius:0;font-size:0rpx;color:transparent;text-align:left;
text-decoration:none;line-height:normal;}
button.form_button::after{content:'';width:0;height:0;-webkit-transform:scale(1);transform:scale(1);display:none;background-color:transparent;}
.button-hover {
color:transparent;
background-color:transparent;
}




