  color:rgba(0, 0, 0, .5);
}
.login-btn{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 220rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  background-color: #000;
  color: #fff;
  font-size: 30rpx;
  margin-top: 36rpx;
}
.right-arrow{
  width: 14rpx;
  height: 26rpx;
  vertical-align: middle;
  margin-left: 12rpx;
}
.welcomes-banner{
  margin-top: 60rpx;
}
.welcomes-banner swiper{
  height: 330rpx!important;
}
.welcomes-banner swiper-item{
  width: 650rpx!important;
