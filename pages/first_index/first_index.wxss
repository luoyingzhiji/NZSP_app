.first-index-logo{
  width: 100%;
  text-align: center;
  margin:10rpx 0 24rpx 0;
  font-size: 0;
}
.first-index-logo image{
  width:250rpx;
  height:74rpx;
}
.indexinfo{
  height:80rpx;
  margin: 0 auto;
  font-size:30rpx;
  font-weight:400;
  color:#333333;
  line-height:46rpx;
  text-align: center;
}
.fanPerson{
  height:28px;
  font-size:20px;
  font-weight:bold;
  color:#000;
  line-height:56rpx;
  text-align: center;
  margin-top: 20px;
}
.indexinfo1{
  width:410rpx;
  margin: 0 auto;
  padding-top: 6px;
  font-size:13px;
  font-weight:300;
  color:rgba(0,0,0,.5);
  display: flex;
  text-align: center;
  align-items: center;
}
.index-down{
  font-size:22rpx;
  font-weight:400;
  color:rgba(0,0,0,.3);
  line-height:32rpx;
  padding-top: 10px;
  padding-bottom: 10px;
  text-align: center;
}
.first-index-banner{
  width: 100%;
  text-align: center;
  margin-top: 10px;
}
swiper-item{
  width: 504rpx!important;
  padding:0 25rpx;
  position: relative;
  left:101rpx;
}

.first-index-banner-list image{
  width: 252px;
  height: 212px;
}
.first-index-banner-list .selected-img{
  width: 24px;
  height: 24px;
  margin-top: 12px;
}
.start-btn{
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  background-color: #fff;
}
.index-btn{
  padding-bottom: 30rpx;
}
.index-btn-content{
  width:650rpx;
  height:100rpx;
  background:#000;
  border-radius:52rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size:30rpx;
  font-weight:bold;
  color:#fff;
  margin: 0 auto;
}
.logo{
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.logo image{
  width: 144px;
  height: 144px;
}