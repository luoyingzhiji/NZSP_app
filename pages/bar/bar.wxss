
@import "../../wxParse/wxParse.wxss";
.txt{
  color: #FFFFFF;
  font-size: 36rpx;
  line-height: 50rpx;
  text-align: center;
  font-weight: 500;
}
.bar-info{
  width: 750rpx;
  background-color:#000;

}
.bar-scroll{
  display:flex;
  flex-wrap: nowrap;

}
.scroll-eee{
  background:rgba(238,238,238,.2);
}
.scroll{
  background-color:#2FBAF5;
}

.scroll:last-child ,.scroll-eee:last-child{
  margin-right: 0!important;
}
